        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:De2.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Quản lý Khách Thuê Nhà" Width="200" Height="40" Margin="10" Click="btnKhach_Click"/>
            <Button Content="Quản lý Hợp Đồng" Width="200" Height="40" Margin="10" Click="btnHopDong_Click"/>
        </StackPanel>
    </Grid>
</Window>
